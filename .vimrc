set nocompatible
execute pathogen#infect()

let g:gruvbox_italic=1
"set termguicolors

colorscheme gruvbox
syntax on
filetype on
filetype indent on
filetype plugin on
set background=dark

set showmode
set cursorline
set number
set hlsearch
set autoindent
set smartindent
set smarttab
set shiftwidth=2
set softtabstop=2
set expandtab
